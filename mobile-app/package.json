    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@expo/metro-config": "~0.18.4",
    "@expo/metro-runtime": "~3.2.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
