{
  "name": "mobile-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "npx expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo run:ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo run:android",
    "generate-themes": "tamagui generate-themes ./theme-builder.ts ./themes.ts",
    "test": "jest --watchAll",
    "storybook-generate": "sb-rn-get-stories",
    "graphql-gen": "graphql-codegen --watch --require dotenv/config --config codegen.ts dotenv_config_path=.env"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
